class Container
    v = none
    i = 0

    fun _init (self)
        self.v = []
    
    fun add (self, a)
        self.v.push (a)
    
    fun _iter (self)
        p = v[i]
        i = i + 1

        if i >= len (v)
            i = 0 # reset
            return? # stop iteration
        return p
    
c = Container ()
c.add (1)
c.add (2)
c.add (3)
c.add (4)
c.add (5)

for i in c
    write (i)