import 'socket' as s
import 'file' as file

a = s.Server ('0.0.0.0', 8000)

fun home ()
    a = file ('server_assets/contact.html', 'r')
    r = a.read ()
    a.close ()

    return {
        'status': 200,
        'Content-Type': 'text/html',
        'body': r
    }

fun contact ()
    return {
        'status': 200,
        'Content-Type': 'text/html',
        'body': '<h1>Contact</h1>'
    }

a.add_get ('/', home)
a.add_get ('/contact', contact)

a.serve ()