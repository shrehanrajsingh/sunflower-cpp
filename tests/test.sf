fun fib (n)
    f1 = 0
    f2 = 1

    while n > 1
        [f1, f2, n] = [f2, f1 + f2, n - 1]
    
    return f2

print (fib (20))

fun hanoi (n, s, a, d)
    if n == 1
        print ('Move disk 1 from', s, 'to', d)
        return
    
    hanoi (n - 1, s, d, a)
    print ('Move disk', n, 'from', s, 'to', d)
    hanoi (n - 1, a, s, d)

hanoi (4, 'S', 'A', 'D')