fun nth_fib (n)
    f1 = 0
    f2 = 1

    while n > 1
        t = f2
        f2 = f1 + f2
        f1 = t
        n = n - 1
    
    return f2

fun fact (n)
    if n == 1
        return n
    return fact (n - 1) * n

print(fact(5), nth_fib(5))

a = nth_fib(20)
print (a)

fun nth_fib_short (n)
    [f1, f2] = [0, 1]

    while n > 1
        [f1, f2, n] = [f2, f1 + f2, n - 1]
    
    return f2

fun nth_fib_r (n)
    if n < 2
        return n
    return nth_fib_r(n - 1) + nth_fib_r(n - 2)

for i in 1 to 20
    print (nth_fib_short(i), nth_fib_r (i))